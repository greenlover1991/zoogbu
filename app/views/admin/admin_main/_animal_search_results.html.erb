	<table id="search_table" align="center">
		<tr class="table_caption"><td colspan=6>ANIMAL SEARCH RESULTS</td></tr>
		<tr class="table_head">
			<td>Name</td>
			<td>Age</td>
			<td>Gender</td>
			<td>Height</td>
			<td>Weight</td>
			<td>Motto</td>
		</tr>
		
	<% for animal in @animals %> 	
		<tr>
			<td> <%= link_to animal.name, admin_animal_path(animal) %> </td>
			<td> <%= animal.age %> </td>
			<td> <%= animal.gender %> </td>
			<td> <%= animal.height %> </td>
			<td> <%= animal.weight %> </td>
			<td> <%= animal.motto %> </td>
		</tr>
	<% end %>
	
</table>